# Unit file for dtemplate_<%= @instance %>

[Unit]
Description=<%= @instance %> dtemplate
After=syslog.target
<% if !@dependencies.empty? %>
Before=<%= @dependencies.join(" ") %>
<% end %>
[Service]
Type=oneshot
Environment='ARGS=-n <%= @instance %> -D <%= @defaults %> -m <%= @mustache %> -s <%= @src %> -d <%= @dest %> -M <%= @mode %> -O <%= @owner %> -G <%= @group %>'
ExecStart=<%= @dtemplate %> $ARGS

User=dtemplate
Group=dtemplate

[Install]
WantedBy=multi-user.target
